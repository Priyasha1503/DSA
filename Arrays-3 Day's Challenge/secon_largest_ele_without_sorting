#https://www.geeksforgeeks.org/problems/second-largest3735/1

class Solution:
    def getSecondLargest(self, arr):
        # Code Here
        ###OPTIMAL APPRAOCH
        largest,sec_largest=-1,-1
        for i in range(0,len(arr)):
            if arr[i]>largest:
                
                sec_largest=largest
                largest=arr[i]
            elif arr[i]>sec_largest and arr[i]!=largest:
                sec_largest=arr[i]
        return sec_largest

                
